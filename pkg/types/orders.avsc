               }
            }
         }
      },
      {
         "name":"itemid",
         "type":{
            "type":"string",
            "arg.properties":{
               "regex":"Item_[1-9][0-9]{0,2}"
            }
         }
      },
      {
         "name":"orderunits",
         "type":{
            "type":"double",
            "arg.properties":{
               "range":{
                  "min":0.1,
                  "max":10.0
               }
            }
         }
      },
      {
