{
   "namespace":"ksql",
   "name":"Orders",
   "type":"record",
   "fields":[
      {
         "name":"ordertime",
         "type":{
            "type":"long",
            "arg.properties":{
               "range":{
                  "min":1487715775521,
                  "max":1519273364600
               }
            }
         }
      },
      {
         "name":"orderid",
         "type":{
            "type":"int",
            "arg.properties":{
